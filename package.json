{
  "name": "ui-style",
  "version": "0.1.0",
  "description": "GOabela UI-Style",
  "main": "compiled/style.css",
  "scripts": {
    "render:dev": "node-sass src/index.scss compiled/style.dev.css --output-style nested --indent-type space --indent-width 1",
    "dev": "npm run render:dev",

    "render:build": "node-sass src/index.scss compiled/style.min.css --output-style compressed",
    "build": "npm run render:build",

    "all": "npm run render:dev && npm run render:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belaarany/ui-style.git"
  },
  "author": "Bela Arany",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/belaarany/ui-style/issues"
  },
  "homepage": "https://github.com/belaarany/ui-style#readme",
  "devDependencies": {
    "node-sass": "^4.8.3"
  }
}
